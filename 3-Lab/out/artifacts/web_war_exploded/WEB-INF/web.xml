    </servlet-mapping>
    <servlet>
        <servlet-name>Result</servlet-name>
        <servlet-class>Result</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Result</servlet-name>
        <url-pattern>/Result</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SerchEngine</servlet-name>
        <servlet-class>SerchEngine</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SerchEngine</servlet-name>
        <url-pattern>/SerchEngine</url-pattern>
    </servlet-mapping>


